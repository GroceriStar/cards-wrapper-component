{
  "name": "@groceristar/cards-wrapper",
  "version": "1.0.21",
  "description": "module to use antd cards between projects",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/GroceriStar/cards-wrapper-component",
  "author": "Arthur Tkachenko",
  "license": "MIT",
  "private": false,
  "scripts": {
    "bundle": "rollup -c",
    "build": "babel src -d dist",
    "build:test": "shx rm -rf && babel src -d dist",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "lint": "standard",
    "zprepublish": "yarn clean && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "code-fix": "standard --fix"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest": "^24.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "shx": "^0.3.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
